{
  "name": "angularjs-protractor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "sleep": "sleep 1",
    "prebuild": "npm run clean",
    "clean": "rimraf -rf ./dist",
    "serve": "npm-run-all build lite-server",
    "serve-gh": "npm-run-all gh-pages bs-server",
    "lite-server": "lite-server -c ./config/lite-server.js",
    "bs-server": "browser-sync -c ./config/bs-config.js start",
    "build": "cross-env NODE_ENV=production npm run webpack -- -p",
    "gh-pages": "cross-env NODE_ENV=ghpages npm run webpack -- -p",
    "start": "concurrently --kill-others 'npm run webpack-dev-server'",
    "lint": "ceslint -c ./config/.eslintrc.js 'config/**/*.js' 'src/**/*.js'",
    "webpack": "webpack --config=./config/webpack.config.babel.js -c --process",
    "prebs-server": "mv -f dist angularjs; mkdir -p dist; mv -f angularjs dist/",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack.config.babel.js --content-base=./src",

    "e2e": "npm run test",
    "pree2e": "npm run webpack",
    "pretest": "webdriver-manager update",
    "test": "concurrently --kill-others -s first 'npm run protractor' 'npm run e2e-server' 'npm run wd-start' -p [{name}] --names 'E2E,HTTP,WD' -c 'yellow,green,cyan'",
    "e2e-server": "lite-server -c ./config/lite-server-e2e.config.js",
    "wd-start": "webdriver-manager start --quiet",
    "protractor": "sleep 5 && protractor ./config/protractor.conf.js",
    "postprotractor": "webdriver-manager shutdown"
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-animate": "1.6.1",
    "angular-aria": "1.6.1",
    "angular-ui-router": "0.4.2",
    "bootstrap": "3.3.7",
    "jquery": "3.1.1"
  },
  "devDependencies": {
    "angular-mocks": "1.6.1",
    "autoprefixer": "6.7.0",
    "babel-cli": "6.22.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "base-href-webpack-plugin": "1.0.0",
    "concurrently": "3.1.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "eslint": "3.14.1",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "faker": "3.1.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.26.0",
    "http-proxy-middleware": "0.17.3",
    "jasmine": "2.5.3",
    "json-loader": "0.5.4",
    "lite-server": "2.2.2",
    "ng-annotate-loader": "0.2.0",
    "ng-cache-loader": "0.0.22",
    "npm-run-all": "4.0.1",
    "postcss-loader": "1.2.2",
    "protractor": "5.0.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "sinon": "1.17.7",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "keywords": [
    "angularjs",
    "webpack"
  ],
  "main": "index.js",
  "author": "daggerok",
  "description": "angularjs quickstart",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angularjs/issues"
  },
  "homepage": "https://github.com/daggerok/angularjs#readme"
}

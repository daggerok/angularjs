{
  "name": "ng-protractor-full-testcycle",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6 <8"
  },
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start": "browser-sync start -c ./config/bs-config.js",
    "wd:stop": "webdriver-manager shutdown",
    "wd:start": "webdriver-manager start --logging=webdriver.log &",
    "pretest": "npm-run-all wd:* > wd.log &",
    "test": "npm-run-all -p start-concurrently run-protractor",
    "bs-e2e-start": "browser-sync start -c ./config/bs-config.js --files=false --no-open --no-notify --no-online --no-ui --no-ghost-mode --no-inject-changes --no-reload-on-restart",
    "start-concurrently": "concurrently --prefix '[{name}]' --names 'HTTP' --prefix-colors 'bgMagenta.bold' 'npm run bs-e2e-start' &",
    "run-protractor": "concurrently --prefix '[{name}]' --names 'E2E TESTS' --prefix-colors 'bgYellow.bold' 'npm-run-all sleep protractor'",
    "sleep": "sleep 2",
    "protractor": "protractor ./config/protractor.js",
    "posttest": "npm-run-all stop:*",
    "stop:wd": "webdriver-manager shutdown",
    "stop:bs": "true|kill $(ps waux|grep 'browser-sync'|grep -v grep|awk '{print $2}')"
  },
  "keywords": [
    "angular",
    "protractor"
  ],
  "author": "daggerok",
  "license": "ISC",
  "devDependencies": {
    "bower": "1.8.0",
    "browser-sync": "2.18.6",
    "concurrently": "3.1.0",
    "connect-history-api-fallback": "1.3.0",
    "npm-run-all": "4.0.0",
    "protractor": "5.0.0"
  },
  "config": {
    "bower_directory": "vendor"
  },
  "dependencies": {
    "angular": "1.6.1",
    "bootstrap": "3.3.7",
    "jquery": "3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/ng-protractor-full-testcycle.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/ng-protractor-full-testcycle/issues"
  },
  "homepage": "https://github.com/daggerok/ng-protractor-full-testcycle#readme"
}

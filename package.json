{
  "scripts": {
    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json ./config/json-server/testdata.babel.js",
    "webpack": "webpack --config=./config/webpack/dev.babel.js -c --process",
    "clean": "rimraf -rf ./dist",
    "sleep": "sleep 3",

    "dev": "npm-run-all -p watch json-server",
    "watch": "npm run webpack -- -w",
    "predev": "npm run clean",

    "semantic": "gulp --gulpfile src/semantic/gulpfile.js",

    "start": "npm-run-all -p webpack-dev-server json-server open-dev-url",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "open-dev-url": "npm-run-all sleep open-dev",
    "open-dev": "open-url localhost:8000",

    "serve": "npm-run-all build start-server-and-open-prod-url",
    "build": "NODE_ENV=production webpack --config=./config/webpack/prod.babel.js -c --process -p",
    "start-server-and-open-prod-url": "npm-run-all -p open-prod-url json-server",
    "open-prod-url": "npm-run-all sleep open-prod",
    "open-prod": "open-url localhost:8080",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "@angular/router": "^0.2.0",
    "jquery": "^3.1.1",
    "semantic-ui": "^2.2.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.22.0",
    "json-server": "^0.8.22",
    "lodash": "^4.16.4",
    "ng-annotate-loader": "^0.2.0",
    "ng-cache-loader": "0.0.21",
    "npm-run-all": "^3.1.0",
    "open-url": "^2.0.2",
    "postcss-loader": "^0.13.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "keywords": [
    "semantic-ui",
    "angularjs",
    "webpack"
  ],
  "private": true,
  "license": "ISC",
  "version": "0.0.1",
  "main": "index.js",
  "author": "daggerok",
  "name": "angularjs-bookmarks",
  "description": "angularjs-bookmarks"
}

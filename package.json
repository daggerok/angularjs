{
  "name": "node-jwt-ng",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install angular bootstrap",
    "start:api-server": "nodemon --delay 1 ./node_modules/babel-cli/bin/babel-node.js server.js -w server.js -w server/**/*.*",
    "start:edge-client": "nodemon --delay 1 ./node_modules/babel-cli/bin/babel-node.js client.js -w client/**/*.* -i client/vendor/**/*.*",
    "start": "npm i; npm-run-all -p start:*"
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "bower": "^1.7.9",
    "cors": "^2.8.0",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "jsonwebtoken": "^7.1.9",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "config": {
    "bower_directory": "client/vendor"
  }
}

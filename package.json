{
  "name": "learn-angular-material",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "precompile": "rimraf -rf ./dist",
    "compile": "webpack --config=./config/webpack.config.babel.js",
    "serve": "npm-run-all build json-server",
    "build": "npm run compile -- --env production",
    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json -r ./config/json-server/routes.json ./config/json-server/testdata.babel.js",
    "gh-pages": "npm run compile -- --env ghpages",
    "dev": "npm-run-all -p watch json-server",
    "watch": "npm run compile -- -w --env development",
    "lint": "eslint -c ./config/.eslintrc.js './config/**/*.js' './src/**/*.js'",
    "start": "npm-run-all -p webpack-dev-server json-server",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack.config.babel.js --env development"
  },
  "dependencies": {
    "angular": "1.6.2",
    "angular-animate": "1.6.2",
    "angular-aria": "1.6.2",
    "angular-material": "1.1.3",
    "angular-ui-router": "0.4.2",
    "normalize.css": "5.0.0",
    "roboto-fontface": "0.7.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.43",
    "angular-mocks": "1.6.2",
    "autoprefixer": "6.7.3",
    "babel-core": "6.23.1",
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "base-href-webpack-plugin": "1.0.2",
    "browser-sync": "2.18.8",
    "compression-webpack-plugin": "0.3.2",
    "concurrently": "3.3.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "editorconfig": "0.13.2",
    "eslint": "3.15.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.1",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "faker": "3.1.0",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.3",
    "json-loader": "0.5.4",
    "json-server": "0.9.5",
    "lite-server": "2.2.2",
    "ng-annotate-loader": "0.2.0",
    "ng-cache-loader": "0.0.23",
    "npm-run-all": "4.0.1",
    "open-url": "2.0.2",
    "postcss": "5.2.13",
    "postcss-loader": "1.3.0",
    "precss": "1.4.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.0.0",
    "rimraf": "2.5.4",
    "rucksack-css": "0.9.1",
    "sinon": "1.17.7",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "stylelint": "7.8.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "syntax-dynamic-import"
    ]
  },
  "keywords": [
    "angularjs",
    "webpack 2"
  ],
  "main": "index.js",
  "author": "daggerok",
  "description": "angularjs material webpack2",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angularjs/issues"
  },
  "homepage": "https://github.com/daggerok/angularjs#readme"
}

{
  "name": "angularjs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "sleep": "sleep 1",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "clean": "rimraf -rf ./dist",
    "serve": "npm-run-all build json-server",
    "dev": "npm-run-all -p watch json-server",
    "start": "npm-run-all -p webpack-dev-server json-server",
    "watch": "webpack -w --config=./config/webpack/dev.babel.js -c --process",
    "build": "NODE_ENV=production webpack --config=./config/webpack/prod.babel.js -c --process -p",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json ./config/json-server/testdata.babel.js"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-server": "^0.8.22",
    "ng-annotate-loader": "^0.2.0",
    "ng-cache-loader": "0.0.21",
    "npm-run-all": "^3.1.0",
    "postcss-loader": "^0.13.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "keywords": [
    "angularjs",
    "webpack"
  ],
  "main": "index.js",
  "author": "daggerok",
  "description": "angularjs",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angularjs/issues"
  },
  "homepage": "https://github.com/daggerok/angularjs#readme"
}

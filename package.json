{
  "name": "learn-angular-material",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "predist": "rimraf -rf ./dist",
    "dist": "webpack --config=./config/webpack/prod.babel.js -c --process -p",
    "serve": "npm-run-all build json-server",
    "build": "cross-env NODE_ENV=production npm run dist",
    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json -r ./config/json-server/routes.json ./config/json-server/testdata.babel.js",
    "gh-pages": "cross-env NODE_ENV=ghpages npm run dist",
    "predev": "npm run clean",
    "dev": "npm-run-all -p watch json-server",
    "watch": "webpack -w --config=./config/webpack/dev.babel.js -c --process",
    "lint": "eslint -c ./config/.eslintrc.js './config/**/*.js' './src/**/*.js'",
    "start": "npm-run-all -p webpack-dev-server json-server",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src"
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-animate": "1.6.1",
    "angular-aria": "1.6.1",
    "angular-material": "1.1.1",
    "angular-ui-router": "0.3.2",
    "normalize.css": "5.0.0",
    "roboto-fontface": "0.6.0"
  },
  "devDependencies": {
    "autoprefixer": "6.6.1",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "base-href-webpack-plugin": "1.0.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "eslint": "3.13.1",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "faker": "3.1.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.26.0",
    "http-proxy-middleware": "0.17.3",
    "json-loader": "0.5.4",
    "json-server": "0.9.4",
    "lite-server": "2.2.2",
    "ng-annotate-loader": "0.2.0",
    "ng-cache-loader": "0.0.22",
    "npm-run-all": "4.0.0",
    "postcss-loader": "1.2.2",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "keywords": [
    "angularjs",
    "webpack"
  ],
  "main": "index.js",
  "author": "daggerok",
  "description": "angularjs quickstart",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angularjs/issues"
  },
  "homepage": "https://github.com/daggerok/angularjs#readme"
}
